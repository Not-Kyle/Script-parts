ScreenGui1A.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame1A.Name = "ScreenGui1A"
Frame1A.Parent = ScreenGui1A
Frame1A.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
Frame1A.BackgroundTransparency = 0.1
Frame1A.BorderColor3 = Color3.fromRGB(0, 0, 255)
Frame1A.BorderSizePixel = 1.7
Frame1A.Position = UDim2.new(0.749766397, 0, 0.121, 0)
Title1A.Size = UDim2.new(0, 215, 0, 30)
Frame1A.Draggable = true
Frame1A.Selectable = true
Frame1A.Active = false

Title1A.Name = "Title1A"
Title1A.Parent = Frame1A
Title1A.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
Title1A.BackgroundTransparency = 0.1
Title1A.BorderColor3 = Color3.fromRGB(0, 0, 255)
Title1A.BorderSizePixel = 1.5
Title1A.Size = UDim2.new(0, 215, 0, 30)
Title1A.Font = Enum.Font.Code
Title1A.Text = "Mikoware | "..game.Players.LocalPlayer.Name
Title1A.TextColor3 = Color3.fromRGB(242, 243, 243)
Title1A.TextSize = 14
Title1A.TextTransparency = 0.1


Frame1A.Visible = false

Frame1A.BackgroundTransparency = 12
Title1A.BackgroundTransparency = 12
Title1A.TextTransparency = 12
Texo1A.TextTransparency = 12
function SCRIPT_VGWH77_FAKESCRIPT() -- Frame.RGB 
    local script = Instance.new('LocalScript')
    script.Parent = Title1A
    function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
 
    counter = 0
 
    while wait(0.1)do
     script.Parent.BorderColor3 = Color3.fromHSV(zigzag(counter),1,1)
 
     counter = counter + 0.01
    end
 
end
coroutine.resume(coroutine.create(SCRIPT_VGWH77_FAKESCRIPT))

