Commands["Serverhop"] = {
    ["Aliases"] = {"serverhop", "shop"};
    ["Function"] = function()
        wait(1)
        local OtherServers = HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
        for _, s in next, OtherServers["data"] do 
            if s ~= game.JobId and s.playing ~= s.maxPlayers then 
                TPService:TeleportToPlaceInstance(game.PlaceId, s["id"])
                 Notify("Mikoware", "Server Hopping..", "", 3)
            end
        end
    end
}
