function SCRIPT_CRRC71_FAKESCRIPT() -- exit.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = exit
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
 
end
coroutine.resume(coroutine.create(SCRIPT_CRRC71_FAKESCRIPT))
function SCRIPT_XUCD69_FAKESCRIPT() -- Aimbot.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Aimbot
	script.Parent.MouseButton1Click:Connect(function()
		print("How to use Team Expo Aimbot")
		print("Click on your enemy with any weapon")
		print("It will begin to automatically shoot your enemy")
		print("To stop it please press   ,")
		local allowed = true
	script.Parent.MouseButton1Click:Connect(function()
		mouse = game.Players.LocalPlayer:getMouse()
	mouse.Button1Down:connect(function()
	if game.Players:GetPlayerFromCharacter(mouse.Target.Parent) then
	local selected = game.Players:GetPlayerFromCharacter(mouse.Target.Parent)
	if game.Players.LocalPlayer.Character:findFirstChildOfClass("Tool") then
	local tool = game.Players.LocalPlayer.Character:findFirstChildOfClass("Tool")
	if tool:findFirstChild("Fire") then
	if selected ~= nil then
		allowed = true
	repeat
	tool.Fire:FireServer(selected.Character.Head.CFrame)
	wait()
	until allowed == false
	end
	end
	end
	end
	mouse.KeyDown:Connect(function(key)
	    if key == "," then
		allowed = false
	    end
	end)
	end)
	end)
	end)
 
end
coroutine.resume(coroutine.create(SCRIPT_XUCD69_FAKESCRIPT))
