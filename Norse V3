local function invis(instance) 
    for i,v in pairs(instance:GetChildren()) do
    if v.Name == "Weld" and v.Parent.Name ~= "Heh" and v.Parent.Name ~= "Barrel" then
        v:Destroy()
    end
    invis(v)
    end
end

