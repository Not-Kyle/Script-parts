local METATABLE = {['LocalPlayer'] = game:GetService('Players').LocalPlayer; ['Run'] = game:GetService("RunService"); ['Mouse'] = game:GetService('Players').LocalPlayer:GetMouse(); ['Noclip'] = false;};

getgenv().UserNotifications = function(title, text, time)
	game.StarterGui:SetCore("SendNotification", {Title = title; Text = text; Icon = 'rbxassetid://13941412659'; Duration = time;})
end

METATABLE['Run'].RenderStepped:connect(function()
    if METATABLE['Noclip'] == true then
        METATABLE['LocalPlayer'].Character.Torso.CanCollide = false
        METATABLE['LocalPlayer'].Character.Head.CanCollide = false
    end
end)

METATABLE['Mouse'].KeyDown:connect(function(noclipkey)
    if noclipkey:lower() == 'x' then
        METATABLE['Noclip'] = not METATABLE['Noclip']
        UserNotifications("Coryu", "Noclip is now "..tostring(METATABLE['Noclip']), 5)
    end
end)

