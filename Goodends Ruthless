Tp.Text = "Target's name"
Tp.TextColor3 = Color3.new(0.666667, 0, 0)
Tp.TextSize = 17
local GetPlayer = function(String)
	local Found = {}
	for i,v in pairs(game:GetService("Players"):GetPlayers()) do
		if v.Name:lower():sub(1, #String) == String:lower() then
			table.insert(Found,v)
		end
	end    
	if #Found == 1 then
		for i,v in pairs(Found) do
			return v
		end
	else
		return nil
	end
end
Tp.FocusLost:Connect(function()
	if GetPlayer(Tp.Text) then
		local c = GetPlayer(Tp.Text)
		local d = game:service'Players'.LocalPlayer
		if not c.Character and not d.Character then return end
		repeat
local camera = workspace.CurrentCamera
local hum = game.Players.LocalPlayer.Character.Humanoid
hum.HipHeight = 30

local target = game.Players[Tp.Text].Character.Torso -- CHANGE THE USERNAME TO WHAT U WANT 


game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(-16,30,0)
wait()
camera.CameraSubject = game.Players[Tp.Text].Character
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(0,35,-13)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(33,30,3)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(-13,30,0)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(0,5,-13)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(-15,5,0)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(0,5,-14)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(-13,30,0)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(13,30,0)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(0,30,13)
wait()
until hum.Health == 0
	end
end)
	end
end)
 
