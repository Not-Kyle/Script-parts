_10.MouseButton1Down:Connect(function()
    Workspace.CurrentCamera.FieldOfView = 10
    Notify("Field Of View","Fov set to 10",nil,5)
end)

discord.MouseButton1Down:Connect(function()  --i love simplistic scripts like this
    setclipboard("cory#9451")
  Notify("Field Of View","Discord has been set to your clipboard",nil,5)
end)

