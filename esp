local Players = game:GetService("Players")
local command = 'esp' --you can change this to whatever you want
local Template = script.SurfaceGui
--// Init
game.Players.PlayerAdded:Connect(function(player)
	player.Chatted:Connect(function(msg)
		if msg == command then
			for _,v in pairs(game.Players:GetChildren()) do					
				if player.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 and not v.Character.Torso:FindFirstChild("SurfaceGui") then
						local TrackTemplate = Template:Clone()
						TrackTemplate.Parent = v.Character.Torso
						TrackTemplate.TextLabel.Text = v.Name
				else  
					if not v.Character.UpperTorso:FindFirstChild("SurfaceGui") then
						local TrackTemplate = Template:Clone()
						TrackTemplate.Parent = v.Character.UpperTorso					
					TrackTemplate.TextLabel.Text = v.Name
					end
				print("ESP Enabled")
				end
			end
		end
	end)
end)
