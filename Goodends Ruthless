game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == ",fix") then
game.Lighting.ColorCorrection.Enabled = false 
end
end)
game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)
if (string.lower(msg) == ",juicewrld") then
game.Lighting.ColorCorrection.Enabled = true 
game.Lighting.COlorCorrection.Saturation = 35
end
end)


local mouse = game.Players.LocalPlayer:GetMouse()
 
mouse.KeyDown:Connect(function(v)
    if v:lower() == "v" then
local part = game.Workspace:FindFirstChild("Part")
game.Players.LocalPlayer.Character.Head.Touched:Connect(function(part)
part.CanCollide = false
wait(3)
part.CanCollide = true 
end)
end
end)
 
game.Players.LocalPlayer.Chatted:Connect(function(msg)
    if string.find(msg,",u ") then 
        local name = string.gsub(msg,",u ", "") 
        for i,v in pairs (game.Players:GetChildren()) do
            if string.sub(string.lower(v.Name) , 1, string.len(name)) == string.lower(name) then 
                game.Players.LocalPlayer.Backpack.Uzi.Parent = game.Players.LocalPlayer.Character
                for i = 1,450 do
                    while true do
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.Torso.CFrame + Vector3.new(0,0,10)
wait()
 
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.Torso.CFrame + Vector3.new(0,0,-10)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =  v.Character.Torso.CFrame + Vector3.new(-10,0, 0)
wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =  v.Character.Torso.CFrame + Vector3.new(10,0,0)
wait()

        game.Players.LocalPlayer.Character.Uzi.Fire:FireServer(v.Character.Head.CFrame)
end
                end
            end
        end
        
    end
end)

game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)

if (string.lower(msg) == ",uzi") then
	repeat
		while true do
			wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace["Uzi | $150"].Head.CFrame * CFrame.new(-2,0,0)
		end
	until game.Players.LocalPlayer.Backpack.Uzi
end
end)
 

game:GetService("Players").LocalPlayer.Chatted:connect(function(msg)

if (string.lower(msg) == ",stam") then
	for i = 1,75 do
		wait()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace["Greenbull | $12"].Head.CFrame * CFrame.new(0,0,-2)
	end
end
end)

