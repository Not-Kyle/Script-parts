local ncall = rm.__namecall

rm.__namecall = newcclosure(function(self, ...)
    if caller() then return ncall(self, ...) end
    local Arguments, Method = {...}, getnamecallmethod() or get_namecall_method();
    if Method == "FireServer" and Arguments[1] == "WalkSpeed" or Arguments[1] == "JumpPower" or Arguments[1] == "HipHeight" then 
        return nil 
    end
    if Method == "FireServer" and tostring(self) == "Input" then 
        if Arguments[1] == "bv" or Arguments[1] == "hb" or Arguments[1] == "ws" then 
            return wait(9e9)
        end
    end
    return ncall(self, unpack(Arguments))
end)
