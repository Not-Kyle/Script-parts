local a, b = pcall(a)
if a then
	print("success")
else
	print("Error: "..b)
	game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("{Russian roulette} - Glock not Detected! Please try again ", "All")
	c:Kick("Are you broke lol buy a fucking Glock it's Russian roulette")
end
wait(6)
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("{Russian roulette} - Glock Detected! Use /[1-6] to insert your bullets ", "All")
local a = nil
local b = "/"
c.Chatted:Connect(function(a)
	local a = a:lower()
	if a:sub(1, 2) == (b.."1") then
		wait(3)
		game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("{Russian roulette} - Amount of bullets detected (1) starting process ", "All")
		bullet1()
	end
end)
c.Chatted:Connect(function(a)
	local a = a:lower()
	if a:sub(1, 2) == (b.."2") then
		wait(3)
		game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("{Russian roulette} - Amount of bullets detected (2) starting process ", "All")
		bullet2()
	end
end)
