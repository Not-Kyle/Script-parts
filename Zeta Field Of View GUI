--script made by coryu, spacedgodcoryu

local Tick = tick()

local function Notify(title, text, icon, time)
    game.StarterGui:SetCore("SendNotification", {
       Title = title;
       Text = text;
       Icon = icon;
       Duration = time;
    })
end

local ScreenGui = Instance.new("ScreenGui")
