	        if v.Character:FindFirstChild("Right Arm") then
	            v.Character:FindFirstChild("Right Arm").ChildAdded:Connect(function(child)
	                if child.Name == "RightGrip" then
	                    wait(0.1)
	                    if child.Part1 ~= nil then
