function SCRIPT_CCAE84_FAKESCRIPT() -- freeuservegan.yuh 
	local script = Instance.new('LocalScript')
	script.Parent = freeuservegan
	loadstring(game:HttpGet("https://pastebin.com/raw/gvvt8Ttr", true))()
 
	game.Players.PlayerAdded:connect(function(plr)
	wait(1)
	    for i = 1,#mains do
		if string.lower(plr.Name) == tostring(string.lower(mains[i])) then
		    game.CoreGui.RobloxGui.PlayerListContainer.ScrollList[plr.Name].BGFrame.PlayerName.TextColor3 = Color3.new(1, 170/255, 0)
	            game.CoreGui.RobloxGui.PlayerListContainer.ScrollList[plr.Name].BGFrame.MembershipIcon.Image = "rbxassetid://157449069"
		end
	    end
	    for i = 1,#special do
		if string.lower(plr.Name) == tostring(string.lower(special[i])) then
	            game.CoreGui.RobloxGui.PlayerListContainer.ScrollList[plr.Name].BGFrame.PlayerName.TextColor3 = Color3.new(0, 255, 255)
	            game.CoreGui.RobloxGui.PlayerListContainer.ScrollList[plr.Name].BGFrame.MembershipIcon.Image = "rbxassetid://279380243"
		end
	    end
	end)
	for i, x in next, game:GetService"Players":GetPlayers() do
	    for i = 1,#mains do
		if string.lower(x.Name) == tostring(string.lower(mains[i])) then
		    game.CoreGui.RobloxGui.PlayerListContainer.ScrollList[x.Name].BGFrame.PlayerName.TextColor3 = Color3.new(1, 170/255, 0)
	            game.CoreGui.RobloxGui.PlayerListContainer.ScrollList[x.Name].BGFrame.MembershipIcon.Image = "rbxassetid://157449069"
		end
	    end
	    for i = 1,#special do
		if string.lower(x.Name) == tostring(string.lower(special[i])) then
		    game.CoreGui.RobloxGui.PlayerListContainer.ScrollList[x.Name].BGFrame.PlayerName.TextColor3 = Color3.new(0, 255, 255)
	            game.CoreGui.RobloxGui.PlayerListContainer.ScrollList[x.Name].BGFrame.MembershipIcon.Image = "rbxassetid://279380243"
		end
	    end
	end
	-- remove this if u think ur gonna be on the blacklist
	for i = 1,#blacklist do
	    if string.lower(game.Players.LocalPlayer.Name) == string.lower(tostring(blacklist[i])) then
		game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('ez',"All")
		game.Players.LocalPlayer:Kick("You are Blacklisted from Vegan GUI V5, probably for pissing us off")
	    end
	end
 
end
coroutine.resume(coroutine.create(SCRIPT_CCAE84_FAKESCRIPT))
function SCRIPT_GHTA87_FAKESCRIPT() -- freeuservegan.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = freeuservegan
	local plr = game.Players.LocalPlayer
	local r = game:GetService('RunService').Heartbeat
	local input = game:GetService('UserInputService')
 
	local speed = 1.5
 
	local x,y,z = 0,0,0
 
	local e = false
 
	plr.Chatted:connect(function(cmd)
		cmd=cmd:lower()
		if cmd == ":speed" then
			e = true
		end
	end)
 
	plr.Chatted:connect(function(cmd)
		cmd=cmd:lower()
		if cmd == ":unspeed" then
			e = false
		end
	end)
 
	input.InputBegan:connect(function(k)
		if k.UserInputType == Enum.UserInputType.Keyboard then
			if k.KeyCode == Enum.KeyCode.W then
				z = -speed	
			end
		end
	end)
 
	input.InputBegan:connect(function(k)
		if k.UserInputType == Enum.UserInputType.Keyboard then
			if k.KeyCode == Enum.KeyCode.S then
				z = speed	
			end
		end
	end)
 
	input.InputBegan:connect(function(k)
		if k.UserInputType == Enum.UserInputType.Keyboard then
			if k.KeyCode == Enum.KeyCode.A then
				x = -speed	
			end
		end
	end)
 
	input.InputBegan:connect(function(k)
		if k.UserInputType == Enum.UserInputType.Keyboard then
			if k.KeyCode == Enum.KeyCode.D then
				x = speed	
			end
		end
	end)
 
	----------
 
	input.InputEnded:connect(function(k)
		if k.UserInputType == Enum.UserInputType.Keyboard then
			if k.KeyCode == Enum.KeyCode.W then
				z = 0
			end
		end
	end)
 
	input.InputEnded:connect(function(k)
		if k.UserInputType == Enum.UserInputType.Keyboard then
			if k.KeyCode == Enum.KeyCode.S then
				z = 0
			end
		end
	end)
 
	input.InputEnded:connect(function(k)
		if k.UserInputType == Enum.UserInputType.Keyboard then
			if k.KeyCode == Enum.KeyCode.A then
				x = 0
			end
		end
	end)
 
	input.InputEnded:connect(function(k)
		if k.UserInputType == Enum.UserInputType.Keyboard then
			if k.KeyCode == Enum.KeyCode.D then
				x = 0
			end
		end
	end)
 
 
	r:connect(function()
		if e == true then
		plr.Character.HumanoidRootPart.CFrame = plr.Character.HumanoidRootPart.CFrame * CFrame.new(x,y,z)
		end
	end)
 
end
coroutine.resume(coroutine.create(SCRIPT_GHTA87_FAKESCRIPT))
